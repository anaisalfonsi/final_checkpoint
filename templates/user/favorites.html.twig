{% extends 'base.html.twig' %}

{% block body %}
    <div class="container-fluid">
        {% for article in articles %}
            <div class="container">
                <ul class="col-12">
                    <li>Id: {{ article.id }}</li>
                    <li>Title: {{ article.title }}</li>
                    <li>Subject: {{ article.subject }}</li>
                </ul>
            </div>
        {% endfor %}
    </div>
    <div class="white-space"></div>
{% endblock %}
